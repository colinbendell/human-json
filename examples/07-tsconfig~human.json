{
  "compilerOptions": {
    "alwaysStrict": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "isolatedDeclarations": true,
    "lib": [ "es2020" ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "newLine": "lf",
    "noImplicitOverride": true,
    "outDir": "../built/local",
    "preserveConstEnums": true,
    "pretty": true,
    "rootDir": ".",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictBindCallApply": false,
    "target": "es2020",
    "types": [],
    "useUnknownInCatchVariables": false
  }
}
