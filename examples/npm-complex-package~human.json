{
  "name": "@example/complex-app",
  "version": "2.5.3",
  "author": { "name": "Example Team", "email": "team@example.com", "url": "https://example.com" },
  "browserslist": {
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"],
    "production": [">0.2%", "not dead", "not op_mini all"]
  },
  "bugs": { "url": "https://github.com/example/complex-app/issues" },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.0",
    "axios": "^1.6.0",
    "classnames": "^2.3.2",
    "date-fns": "^3.0.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.0",
    "react-router-dom": "^6.20.0",
    "redux": "^5.0.0",
    "zod": "^3.22.0"
  },
  "description": "A complex enterprise application",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "engines": { "node": ">=18.0.0", "npm": ">=9.0.0" },
  "homepage": "https://github.com/example/complex-app#readme",
  "jest": {
    "collectCoverageFrom": ["src/**/*.{ts,tsx}", "!src/**/*.d.ts", "!src/**/*.stories.tsx"],
    "coverageThreshold": { "global": { "branches": 80, "functions": 80, "lines": 80, "statements": 80 } },
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "keywords": ["react", "typescript", "webpack", "enterprise", "ui-framework"],
  "license": "MIT",
  "lint-staged": { "*.{json,css,md}": ["prettier --write"], "*.{ts,tsx,js,jsx}": ["eslint --fix", "prettier --write"] },
  "main": "dist/index.js",
  "peerDependencies": { "react": ">=18.0.0", "react-dom": ">=18.0.0" },
  "private": false,
  "repository": { "type": "git", "url": "https://github.com/example/complex-app.git" },
  "scripts": {
    "build": "npm run clean && npm run build:types && webpack --mode production",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf dist coverage",
    "dev": "webpack serve --mode development",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && npm test",
    "test": "jest --coverage",
    "test:e2e": "playwright test",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "types": "dist/index.d.ts"
}
